{
  "name": "meny_react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate_prisma": "npm run gen_MFD && npm run gen_NAL && npm run gen_CHR && npm run gen_DRW",
    "gen_MFD": "prisma generate --schema=prisma/schema-MFD.prisma && prisma db push --schema=prisma/schema-MFD.prisma",
    "gen_NAL": "prisma generate --schema=prisma/schema-NAL.prisma && prisma db push --schema=prisma/schema-NAL.prisma",
    "gen_CHR": "prisma generate --schema=prisma/schema-CHR.prisma && prisma db push --schema=prisma/schema-CHR.prisma",
    "gen_DRW": "prisma generate --schema=prisma/schema-DRW.prisma && prisma db push --schema=prisma/schema-DRW.prisma",
    "dev": "npm run generate_prisma && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run generate_prisma && npm run build"
  },
  "dependencies": {
    "@fontsource/source-sans-pro": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mapbox/mapbox-gl-draw": "^1.5.0",
    "@nextui-org/slider": "^2.2.15",
    "@nextui-org/system": "^2.2.5",
    "@nextui-org/theme": "^2.2.9",
    "@prisma/client": "^6.5.0",
    "@types/redis": "^4.0.10",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookies-next": "^4.3.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.5.4",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.6.0",
    "mapbox-gl-compare": "^0.4.1",
    "moment": "^2.30.1",
    "next": "^14.2.10",
    "react": "^18",
    "react-colorful": "^5.6.1",
    "react-dom": "^18",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.28.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/mapbox__mapbox-gl-draw": "^1.4.8",
    "@types/mapbox-gl": "^3.4.0",
    "@types/node": "^20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18",
    "@types/react-modal": "^3.16.3",
    "@types/react-transition-group": "^4.4.11",
    "eslint": "^8",
    "eslint-config-next": "14.2.10",
    "postcss": "^8",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}